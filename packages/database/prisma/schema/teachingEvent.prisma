model TeachingEvent {
  id          Int       @id @default(autoincrement())
  name        String
  semester    SemesterPeriod @relation(fields: [semesterPeriodId], references: [id])
  semesterPeriodId Int
  ordered     Boolean
  hours       Int
  program     Program?   @relation(fields: [programId], references: [id])
  programId   Int?
}