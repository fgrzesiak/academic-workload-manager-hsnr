model Teacher {
  id                 Int      @id @default(autoincrement())
  username           String   @unique
  password           String
  firstName          String
  lastName           String
  retirementDate     DateTime
  totalTeachingDuty  Float
  teachingGroups     TeachingGroup[]
  teachingDuties     TeachingDutyPerSemester[]
  discounts          Discount[]
  supervisions       Supervision[]
}